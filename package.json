{
  "name": "jaytech-rn-ui",
  "version": "0.0.31",
  "main": "./lib/index.js",
  "files": [
    "lib/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/galio-org/galio.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/galio-org/galio/issues"
